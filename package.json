{
  "name": "@clean-stack/source",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "nx reset && nx run-many --target=clean --all --parallel && rm -rf dist coverage tmp .nx docs apps/clean-docs/.docusaurus",
    "dep-graph": "nx graph",
    "format": "prettier --write \"**/*.{ts,js,json,md,tsx,jsx}\"",
    "generate:docs": "typedoc --options typedoc.json && http-server docs -p 8888 -o",
    "lint": "nx run-many --target=lint --all --parallel",
    "lint:fix": "nx run-many --target=lint --fix --all --parallel",
    "platform:all": "npm run platform:observability && npm run platform:cache",
    "platform:all:down": "npm run platform:observability:down && npm run platform:cache:down",
    "platform:cache": "docker compose -f PLATFORM_SETUP/cache_stack/docker-compose.yml up -d && open http://localhost:13333",
    "platform:cache:down": "docker compose -f PLATFORM_SETUP/cache_stack/docker-compose.yml down",
    "platform:observability": "docker compose -f PLATFORM_SETUP/observability_stack/docker-compose.yml up -d && open http://localhost:3000",
    "platform:observability:down": "docker compose -f PLATFORM_SETUP/observability_stack/docker-compose.yml down",
    "mongodb:up": "docker compose -f PLATFORM_SETUP/mongo_stack/docker-compose.yml up -d",
    "mongodb:down": "docker compose -f PLATFORM_SETUP/mongo_stack/docker-compose.yml down",
    "test": "rm -rf coverage && npm run clean && nx run-many --target=test --all --parallel",
    "test:merge:coverage": "npm run test && node ./tools/merge-coverage.js"
  },
  "dependencies": {
    "@docusaurus/core": "3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@docusaurus/theme-mermaid": "3.4.0",
    "@grpc/grpc-js": "^1.12.2",
    "@koa/bodyparser": "^5.1.1",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^13.1.0",
    "@mdx-js/react": "^3.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.1",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.53.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "clsx": "^2.1.1",
    "http-status-codes": "^2.3.0",
    "koa": "^2.15.3",
    "koa-helmet": "^7.0.2",
    "mongoose": "^8.8.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^4.7.0",
    "tslib": "^2.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "@nx-extend/docusaurus": "^3.1.0",
    "@nx/esbuild": "20.0.10",
    "@nx/eslint": "20.0.10",
    "@nx/eslint-plugin": "20.0.10",
    "@nx/js": "20.0.10",
    "@nx/node": "20.0.10",
    "@nx/vite": "20.0.10",
    "@nx/web": "20.0.10",
    "@nx/workspace": "20.0.10",
    "@swc-node/register": "~1.9.2",
    "@swc/core": "~1.5.29",
    "@swc/helpers": "~0.5.13",
    "@types/koa": "^2.15.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "2.1.4",
    "@vitest/ui": "^1.6.0",
    "docusaurus-plugin-typedoc": "^1.0.5",
    "esbuild": "^0.19.12",
    "eslint": "~8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^3.0.1",
    "grpc-tools": "^1.12.4",
    "http-server": "^14.1.1",
    "mongodb-memory-server": "^10.1.2",
    "nx": "20.0.10",
    "nx-github-pages": "^1.0.0",
    "nyc": "^17.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^4.1.0",
    "supertest": "^7.0.0",
    "ts-proto": "^2.2.3",
    "typedoc": "^0.26.8",
    "typedoc-material-theme": "^1.1.0",
    "typescript": "~5.5.4",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
